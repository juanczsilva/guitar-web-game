{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": "off",
        "eqeqeq": "off",
        "comma-dangle": "off",
        "no-param-reassign": "off",
        "no-console": "off",
        "prefer-destructuring": "off",
        "no-lonely-if": "off",
        "no-use-before-define": "off",
        "max-len": "off"
    }
}
